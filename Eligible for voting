#include<stdio.h>
int main()
{
     int age;
     printf("Enter Age of Person : ");
     scanf("%d",&age);
     if(age>17)
    {
	  printf("\nPerson is Eligible for Voting");
    }
     else
    {
		       printf("\nPerson is NOT Eligible for Voting");
          printf("\nPerson is Eligible for voting after %d years",18-age);
    }
	 return 0;
}
